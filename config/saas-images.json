{
  "images": [
    {
      "id": "prestashop",
      "name": "PrestaShop",
      "imageName": "prestashop/prestashop:8",
      "exposedPort": 80,
      "hostPort": 8086,
      "description": "Open-source ecommerce platform for testing",
      "env": [
        "DB_SERVER=prestashop-mysql",
        "DB_NAME=prestashop",
        "DB_USER=prestashop",
        "DB_PASSWD=prestashop_pass",
        "PS_INSTALL_AUTO=1",
        "PS_DEV_MODE=0",
        "PS_DOMAIN={PUBLIC_HOST}:8086",
        "PS_FOLDER_ADMIN=admin",
        "PS_FOLDER_INSTALL=install",
        "ADMIN_MAIL=admin@prestashop.com",
        "ADMIN_PASSWD=admin123"
      ],
      "waitTime": 120000,
      "healthCheck": "/",
      "dependencies": [
        {
          "id": "prestashop-mysql",
          "imageName": "mariadb:10.11",
          "exposedPort": 3306,
          "env": [
            "MYSQL_ROOT_PASSWORD=admin",
            "MYSQL_DATABASE=prestashop",
            "MYSQL_USER=prestashop",
            "MYSQL_PASSWORD=prestashop_pass"
          ],
          "waitTime": 30000,
          "healthCheck": null
        }
      ]
    },
    {
      "id": "filebrowser",
      "name": "FileBrowser",
      "imageName": "filebrowser/filebrowser:latest",
      "exposedPort": 8080,
      "hostPort": 8087,
      "description": "Web-based file manager with upload, download, and preview support",
      "env": [],
      "entrypoint": ["sh", "-c"],
      "command": ["filebrowser config init -a 0.0.0.0 -p 8080 -d /database/filebrowser.db -r /srv && filebrowser users add admin admin12345678 --perm.admin -d /database/filebrowser.db && exec filebrowser -a 0.0.0.0 -p 8080 -d /database/filebrowser.db -r /srv"],
      "credentials": {"username": "admin", "password": "admin12345678"},
      "waitTime": 5000,
      "healthCheck": "/health"
    },
    {
      "id": "focalboard",
      "name": "Focalboard",
      "imageName": "mattermost/focalboard:latest",
      "exposedPort": 8000,
      "hostPort": 8088,
      "description": "Open-source task and project management tool with board, table, and calendar views",
      "env": [],
      "entrypoint": ["bash", "-c"],
      "command": ["/opt/focalboard/bin/focalboard-server & sleep 5; exec 3<>/dev/tcp/localhost/8000 && printf 'POST /api/v2/register HTTP/1.1\\r\\nHost: localhost:8000\\r\\nContent-Type: application/json\\r\\nX-Requested-With: XMLHttpRequest\\r\\nContent-Length: 72\\r\\nConnection: close\\r\\n\\r\\n{\"username\":\"admin\",\"email\":\"admin@test.com\",\"password\":\"admin12345678\"}' >&3 && cat <&3 && exec 3>&-; wait"],
      "credentials": {"username": "admin", "password": "admin12345678"},
      "waitTime": 15000,
      "healthCheck": "/"
    },
    {
      "id": "centrifugo",
      "name": "Centrifugo",
      "imageName": "centrifugo/centrifugo:v6",
      "exposedPort": 8000,
      "hostPort": 8089,
      "description": "Real-time messaging server supporting WebSocket, SSE, HTTP-streaming, and gRPC",
      "env": [
        "CENTRIFUGO_ADMIN_ENABLED=true",
        "CENTRIFUGO_ADMIN_INSECURE=true",
        "CENTRIFUGO_CLIENT_INSECURE=true",
        "CENTRIFUGO_HTTP_API_INSECURE=true",
        "CENTRIFUGO_CHANNEL_WITHOUT_NAMESPACE_ALLOW_SUBSCRIBE_FOR_CLIENT=true",
        "CENTRIFUGO_CHANNEL_WITHOUT_NAMESPACE_ALLOW_PUBLISH_FOR_SUBSCRIBER=true",
        "CENTRIFUGO_CHANNEL_WITHOUT_NAMESPACE_ALLOW_PRESENCE_FOR_SUBSCRIBER=true",
        "CENTRIFUGO_CHANNEL_WITHOUT_NAMESPACE_ALLOW_HISTORY_FOR_SUBSCRIBER=true",
        "CENTRIFUGO_SSE_ENABLED=true",
        "CENTRIFUGO_HTTP_STREAM_ENABLED=true",
        "CENTRIFUGO_UNI_SSE_ENABLED=true",
        "CENTRIFUGO_UNI_HTTP_STREAM_ENABLED=true",
        "CENTRIFUGO_GRPC_API_ENABLED=true",
        "CENTRIFUGO_CLIENT_ALLOWED_ORIGINS=*"
      ],
      "waitTime": 5000,
      "healthCheck": "/"
    },
    {
      "id": "docuseal",
      "name": "DocuSeal",
      "imageName": "docuseal/docuseal:latest",
      "exposedPort": 3000,
      "hostPort": 8090,
      "description": "Open-source document signing platform for testing e-signature workflows",
      "credentials": {
        "email": "admin@example.com",
        "password": "admin12345678"
      },
      "env": [],
      "waitTime": 15000,
      "healthCheck": "/"
    },
    {
      "id": "excalidraw",
      "name": "Excalidraw",
      "imageName": "excalidraw/excalidraw:latest",
      "exposedPort": 80,
      "hostPort": 8091,
      "description": "Open-source whiteboard for drawing and diagramming (single-user in self-hosted mode)",
      "env": [],
      "waitTime": 10000,
      "healthCheck": "/"
    }
  ]
}
